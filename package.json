{
  "name": "be-kvetching",
  "version": "0.0.0",
  "description": "Turns the tag it adorns into a web component that inherits from [k-fetch](https://github.com/bahrus/k-fetch)",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements",
    "JSON",
    "bind",
    "observe"
  ],
  "main": "be-kvetching.js",
  "module": "be-kvetching.js",
  "exports":{
    ".": "./be-kvetching.js",
    "./behivior": "./behivior.js",
    "./be-kvetching.js": "./be-kvetching.js"
  },
  "files": [
    "*.js",
    "types.d.ts"
  ],
  "types": "types.d.ts",
  "dependencies": {
    "be-enhanced": "0.0.69",
    "be-hive": "0.0.138",
    "k-fetch": "0.0.6",
    "trans-render": "0.0.742",
    "xtal-element": "0.0.598"
  },
  "devDependencies": {
    "@playwright/test": "1.42.1",
    "may-it-serve": "0.0.6"
  },
  "scripts": {
    "serve": "node node_modules/may-it-serve",
    "test": "playwright test",
    "safari": "npx playwright wk http://localhost:3030",
    "update": "ncu -u && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/be-kvetching.git"
  },
