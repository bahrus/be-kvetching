{
  "name": "be-kvetching",
  "version": "0.0.3",
  "description": "Turns the tag it adorns into a web component that inherits from [k-fetch](https://github.com/bahrus/k-fetch)",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements",
    "JSON",
    "bind",
    "observe"
  ],
  "main": "be-kvetching.js",
  "module": "be-kvetching.js",
  "exports": {
    ".": "./be-kvetching.js",
    "./behivior": "./behivior.js",
    "./be-kvetching.js": "./be-kvetching.js"
  },
  "files": [
    "*.js",
    "types.d.ts"
  ],
  "types": "types.d.ts",
  "dependencies": {
    "be-enhanced": "0.0.147",
    "be-hive": "0.0.227",
    "k-fetch": "0.0.12",
    "trans-render": "0.0.848"
  },
  "devDependencies": {
    "@playwright/test": "1.48.0",
    "ssi-server": "0.0.1"
  },
  "scripts": {
    "serve": "python3 ./node_modules/ssi-server/ssi_server.py",
    "test": "playwright test",
    "safari": "npx playwright wk http://localhost:3030",
    "update": "ncu -u && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/be-kvetching.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT"
}
